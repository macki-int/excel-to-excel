<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_RIGHT" prefHeight="500.0" prefWidth="575.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.trollsysstems.exceltoexcel.ExcelToExcelController">
   <TabPane prefHeight="449.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
     <tabs>
       <Tab text="Import">
         <content>
           <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="441.0" prefWidth="1000.0">
                  <children>
                     <TableView fx:id="TableViewConfig" layoutX="8.0" layoutY="9.0" prefHeight="403.0" prefWidth="375.0" AnchorPane.bottomAnchor="0.0">
                       <columns>
                         <TableColumn prefWidth="89.0" text="Name" />
                         <TableColumn prefWidth="124.0" text="Ilość inklinometrów" />
                           <TableColumn minWidth="2.0" text="Data start" />
                           <TableColumn text="Data stop" />
                       </columns>
                     </TableView>
                     <Button layoutX="424.0" layoutY="44.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="150.0" text="Import" AnchorPane.rightAnchor="20.0" />
                     <Button fx:id="buttonOpenConfigDialog" layoutX="424.0" layoutY="9.0" mnemonicParsing="false" onAction="#onButtonOpenConfigDialogClick" prefHeight="25.0" prefWidth="150.0" text="Plik konfiguracyjny" AnchorPane.rightAnchor="20.0" />
                     <Label fx:id="labelPath" layoutX="405.0" layoutY="88.0" prefHeight="89.0" prefWidth="150.0" AnchorPane.rightAnchor="20.0" />
                  </children>
               </AnchorPane>
         </content>
       </Tab>
       <Tab text="Wynik">
         <content>
           <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
         </content>
       </Tab>
     </tabs>
   </TabPane>

    <Label fx:id="welcomeText" />
   <Button fx:id="buttonExit" contentDisplay="TOP" mnemonicParsing="false" onAction="#onButtonExitClick" prefHeight="25.0" prefWidth="150.0" text="Zamknij">
      <VBox.margin>
         <Insets bottom="20.0" right="20.0" />
      </VBox.margin>
   </Button>
</VBox>
